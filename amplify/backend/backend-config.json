{
  "api": {
    "lora": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "lora"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    },
    "tinyassettrackerampl": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "tinyassettrackerampl"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "AMAZON_COGNITO_USER_POOLS",
              "userPoolConfig": {
                "userPoolId": "authtinyassettrackerampl"
              }
            }
          ],
          "defaultAuthentication": {
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7
            },
            "authenticationType": "API_KEY"
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "tinyassettrackerampl": {
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito",
      "serviceType": "managed"
    }
  },
  "function": {
    "lora": {
      "build": true,
      "dependsOn": [
        {
          "attributeEnvMap": {
            "Arn": "TABLE_ARN",
            "Name": "TABLE_NAME"
          },
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "storage",
          "resourceName": "assetdata"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "assetdata": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}